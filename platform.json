{
  "name": "cs-microapps",
  "title": "Crownstone Mircoapps",
  "description": "Upload microapps to Crownstone's opperatingsystem Bluenet.",
  "homepage": "https://crownstone.rocks",
  "license": "",
  "keywords": ["Crownstone", "Bluenet","Microapp"],
  "repository": {
    "type": "git",
    "url": "https://github.com/mPlagge/platform-crownstone-microapps.git"
  },
  "version": "0.0.0",
  "frameworks": {
    "arduino": {
      "package": "framework-cs-arduino",
      "script": "builder/frameworks/cs_arduino.py"
    }
  },
  "packages": {
      "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "owner": "platformio",
      "version": ">=1.60301.0,<1.80000.0",
      "optionalVersions": [
        "~1.80201.0",
        "~1.90201.0"
      ]
    },
      "tool-sreccat": {
      "owner": "platformio",
      "version": "~1.164.0"
    },
      "tool-nrfjprog": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~1.90702.0"
    },
    "framework-cs-arduino": {
      "type": "framework",
      "optional": true,
      "owner": "Crownstone",
      "version": "0.0.0"
    }
  }
}
